    "vitest": "^0.32.0",
    "vitest-fetch-mock": "^0.2.2"
  },
  "type": "module",
  "dependencies": {
    "@fontsource-variable/montserrat": "^5.0.3",
    "@fontsource-variable/open-sans": "^5.0.3",
    "@fontsource-variable/podkova": "^5.0.2"
