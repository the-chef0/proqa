    "@sveltejs/kit": "^1.20.2",
    "@types/bootstrap": "^5.2.6",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "@vitest/coverage-v8": "^0.32.2",
    "bootstrap": "5.1.x",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsdoc": "^46.2.6",
    "eslint-plugin-svelte": "^2.30.0",
    "jsdom": "^22.1.0",
    "msw": "^1.2.2",
